    "@testing-library/svelte": "^5.3.1",
    "@vitest/coverage-v8": "^4.0.17",
    "drizzle-kit": "^0.31.8",
    "jsdom": "^27.4.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
